<html>
  <head>
	<%= javascript_include_tag "prototype" %>
	<%= stylesheet_link_tag "cohort" %>
	<%= stylesheet_link_tag 'style' %>
	    <script src="/javascripts/jquery-1.3.2.min.js" type="text/javascript"></script>
    <script type="text/javascript">
      jQuery(document).ready(function($) {    
        $('#logout').click(function() { 
                window.location = '/authentication';
            })
	});

	</script>
	<style>

	  table {
		border-style:none;
		border-width:1px;
		width:99% !important;
	  }

	  th {
		border-style:none;
		border-width:1px;
	  }

	  td {
		text-align:center;
		border-style:solid;
		border-width:1px;
	  }

	  .row_break td {
		border-width:0px;
	  }

	  div.scrollTableContainer {
		border-style:solid;
		height:68%;
	  }

	  .survival-analysis th, .survival-analysis td {
		text-align:left;
	  }

	  .clinic_own_data {
		position:relative;
		float:left;
		display:block;
		padding-left:10px;
	  }

	  .checked_data {
		position:relative;
		float:right;
		display:block;
		padding-right:10px;
	  }
	  a{
		text-decoration: none !important;
		color: #E8EAF1;
	  }
	  a:hover {
		color:black;
	  }
	  .clinic_data_value:hover {
		color:black;
	  }
	  .checked_data_value:hover{
		color:black;
	  }
	  td.create_button:hover{
		color:black;
	  }
	  .clinic_data_value {
		position:relative;
		float:left;
		display:block;
		border: 0px 0px 2px 0px;
		border-top: 1px outset black;
		border-bottom: 1px outset black;
		border-radius: 7px 7px 7px 7px;
		border-left:0px;
		margin-top:2px;
		border-style: outset;
		width:30%;
		height:100%;
		padding: 10px 16px 10px 10px;
		text-align: center;
		text-decoration: none !important;
		background-color: #7784AE;
		color: #E8EAF1;
		display: block;
		font-size:13px;
	  }
	  .create_button a{
		background-color: #7784AE;
		border-bottom: 1px outset black;
		border-radius: 7px 7px 7px 7px;
		border-style: outset;
		border-top: 1px outset black;
		color: #E8EAF1;
		display: block;
		padding: 10px 16px 10px 10px;
		text-align: center;
		text-decoration: none !important;
		font-size:13px;
	  }
	  .checked_data_value {
		position:relative;
		float:right;
		display:block;
		border-right:0px;
		border-left:0px;
		margin-top:2px;
		margin-top:2px;
		width:20%;
		height:100%;
		display: block;
		font-size:13px;
	  }

	  td.left_top_right {
		border-width:3px 3px 0px 3px;
	  }

	  td.right_bottom_left {
		border-width:1px 3px 3px 3px;
	  }

	  td.left_right {
		border-width:1px 3px 0px 3px;
	  }

	  td.left_top_right_bottom {
		border-width:3px 3px 3px 3px;
	  }

	  .regimen {
		position:absolute;
		top:0px;
		left:1px;
		border-radius:100px;
		background:white;
		color:black;
		text-align:center;
		font-size:15px;
	  }
	  td.regimen_name_top {
		text-align:left;
		border-width:1px 0px 0px 0px;
		padding-left:100px;
	  }

	  td.regimen_name {
		text-align:left;
		border-width:0px 0px 0px 0px;
		padding-left:100px;
	  }

	  td.regimen_name_bottom {
		text-align:left;
		border-width:0px 0px 1px 0px;
		padding-left:100px;
	  }

	  b {
		position:relative;
		float:left;
		display:block;
		width:68px;
		padding-left:5px;
	  }
	  tr{
		width:100% !important;
	  }
	  td.bg {
		background-color:lightgrey;
		-moz-user-select:none;
	  }

	  .logo {
		float:left;
		height: 8em;
	  }
	  .dataTable{
		width:6%
	  }

	</style>

	<title></title>
  </head>
  <body>

	<h1>Mwasibale Motors</h1>
	<h2>Customer Statistics Report</h2>
	<div align="right" style="top:0%;left:0%;">
		  <select class="btn2">
				  <option>Filter Report</option>
				  <option>Oustanding</option>
				  <option>Overdue</option>
				  <option>Fully settled</option>
		  </select>
	</div>
	<div class='scrollTableContainer'>
	  <table cellspacing='0'>
		<thead>
		  <tr>
			
			<td width="35%"style = "text-align:left;" class="bg">Customer details</td>
			<td width="19%" class="bg"><span class="clinic_own_data" > Property</span> <span class="checked_data" >Model </span></td>
				  <td width="0.01%" class="bg" >&nbsp;</td>
				  <td width="19%" class="bg" >   <span class="clinic_own_data"> Charge </span> <span class="checked_data"  >Balance </span></td>
				  <td width="0.01%" class="bg" >&nbsp;</td>
				  <td width="19%" class="bg" >   <span class="clinic_own_data"> Date <br /> brought in  </span> <span class="checked_data"  >Date <br>expected out </span></td>
				  <td width="0.01%" class="bg" >&nbsp;</td>
				  <td width="19%" class="bg" >  <span class="clinic_own_data"> Status  </span></td>
				  </tr>
						</thead>
						<tbody>
						  <tr>
							
							<td style="text-align:left;"></td>
							<td class="left_top_right_bottom" ><span class="checked_data_value">&nbsp;</span></td>
							<td style="border-width:0px 0px 0px 0px">&nbsp;</td>
							<td class="left_top_right_bottom" ><span class="checked_data_value">&nbsp;</span></td>
							<td style="border-width:0px 0px 0px 0px">&nbsp;</td>
							<td class="left_top_right_bottom" ><span class="checked_data_value">&nbsp;</span></td>
							<td style="border-width:0px 0px 0px 0px">&nbsp;</td>
							<td class="left_top_right_bottom" ><span class="checked_data_value">&nbsp;</span></td>
						  </tr>

						  <tr class = 'row_break'>
							<td>&nbsp;</td>
							<td>&nbsp;</td>
							<td>&nbsp;</td>
							<td>&nbsp;</td>
							<td>&nbsp;</td>
						  </tr>

						  

		  </tbody>
	  </table>
	</div>

  </body>
</html>
